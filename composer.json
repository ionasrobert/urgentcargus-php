{
    "name": "mnib-dev/urgentcargus-php",
    "description": "A very simple PHP Wrapper for the UrgentCargus API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MNIB\\UrgentCargus\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
